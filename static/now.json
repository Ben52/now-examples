{
	"version": 2,
	"name": "static",
	"builds": [
		{ "src": "index.html", "use": "@now/html-minifier" },
		{ "src": "styles.css", "use": "@now/html-minifier" },
		{ "src": "favicon.ico", "use": "@now/static" }
